{
  "name": "@fwkui/x-css",
  "version": "1.0.9",
  "description": "A fast, modular CSS-in-JS library with utility-first approach and static extraction support.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "DICTIONARY.md",
    "ROADMAP.md",
